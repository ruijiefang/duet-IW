(declare-const |havoc:8| Int)
(declare-const |x':28:34| Int)
(declare-const |K:32:33| Int)
(assert (and (or (not (<= (- |K:32:33|) 0))
                   (= (+ |x':28:34| (* -2 |K:32:33|)) 0))
               (or (and (= |K:32:33| 0) (= (- |x':28:34|) 0))
                     (and (<= (+ (- |K:32:33|) 1) 0)
                            (<= (+ (- |havoc:8|) 1) 0)
                            (<= (+ (- |havoc:8|) |x':28:34| -1) 0)
                            (<= (+ (- |x':28:34|) 2) 0)))
               (<= (- |K:32:33|) 0) (<= (- |x':28:34|) 0)
               (<= (+ |havoc:8| (- |x':28:34|)) 0)
               (not (= (mod |x':28:34| 2) 0))))
(check-sat)
