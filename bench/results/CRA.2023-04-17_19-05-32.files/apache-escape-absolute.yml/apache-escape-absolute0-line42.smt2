(declare-const havoc Int)
(declare-const |havoc:2| Int)
(declare-const |havoc:1| Int)
(assert (and (<= (+ havoc -1000000) 0) (<= (+ (- havoc) -1000000) 0)
               (<= (+ |havoc:1| -1000000) 0)
               (<= (+ (- |havoc:1|) -1000000) 0)
               (<= (+ |havoc:2| -1000000) 0)
               (<= (+ (- |havoc:2|) -1000000) 0) (< (- havoc) 0)
               (< (- |havoc:1|) 0) (<= (- |havoc:2|) 0) (not (= |havoc:2| 0))
               (<= (+ |havoc:2| (- havoc) 1) 0)
               (not (< (+ |havoc:2| (- havoc) -1) 0))))
(check-sat)
